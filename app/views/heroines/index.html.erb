<h2>All Heroines</h2>

<p> Search For A POWER<br>
  <%= form_tag(heroines_path, method: :get) do %>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search"%>
    <% end %>
</p>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
